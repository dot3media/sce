{
    "name": "d3m/sce",
    "type": "typo3-cms-extension",
    "description": "Create Simple Content Elements for TYPO3",
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-backend": "^9.5 || ^10.4 || ^11.5",
        "typo3/cms-fluid-styled-content": "^9.5 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "D3M\\Sce\\": "Classes/"
        }
    },
    "replace": {
		"typo3-ter/sce": "self.version"
	},
    "extra": {
        "typo3/cms": {
            "extension-key": "sce"
        }
    }
}
